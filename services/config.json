{
    "applications": {
        "someservice": {
            "type": "php",
            "processes": {
                "max": 50,
                "spare": 50
            },
            "root": "var/www/html/course/public",
            "index": "index.php",
            "script": "index.php",
            "options": {
                "file": "/etc/php/7.4/cli/php.ini"
            }
        }
    },
    "listeners": {
        "*:8080": {
            "pass": "applications/someservice"
        }
    }
}
